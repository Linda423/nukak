{
  "version": "0.3.4",
  "useWorkspaces": true,
  "npmClient": "yarn",
  "npmClientArgs": ["--no-package-lock"],
  "command": {
    "publish": {
      "contents": "dist",
      "conventionalCommits": true,
      "gitReset": false,
      "changelog": false,
      "yes": true,
      "amend": true,
      "forceGitTag": true
    },
    "version": {
      "yes": true,
      "amend": true
    }
  }
}
