{
  "version": "0.0.0",
  "useWorkspaces": true,
  "command": {
    "version": {
      "allowBranch": "master"
    },
    "publish": {
      "ignoreChanges": ["*.md", "*.json", "*.config.js", "*.spec.ts", "*.it.ts"]
    }
  },
  "npmClient": "yarn",
  "npmClientArgs": ["--no-package-lock"],
  "packages": ["packages/*"],
  "contents": "dist"
}
