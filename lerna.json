{
  "version": "1.0.2",
  "useWorkspaces": true,
  "npmClient": "yarn",
  "npmClientArgs": [
    "--no-package-lock"
  ],
  "command": {
    "publish": {
      "contents": "dist",
      "conventionalCommits": true,
      "changelog": false,
      "yes": true,
      "forceGitTag": true,
      "forcePublish": true,
      "beta": true
    },
    "version": {
      "yes": true
    }
  }
}
